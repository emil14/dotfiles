  "debug.console.fontFamily": "Terminus (TTF)",
  "debug.console.fontSize": 16,
  "debug.internalConsoleOptions": "openOnSessionStart",
  "debug.javascript.usePreview": true,
  "debug.openDebug": "openOnSessionStart",
  "debug.showBreakpointsInOverviewRuler": true,
  "debug.showInStatusBar": "always",
  "debug.toolBarLocation": "docked",
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": true,
  "docker.containers.description": ["ContainerId", "Ports", "Status"],
  "docker.containers.groupBy": "Compose Project Name",
  "docker.containers.label": "ContainerName",
  "docker.containers.sortBy": "Label",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.definitionLinkOpensInPeek": false,
  "editor.foldingHighlight": false,
  "editor.fontFamily": "Hermit, Arial",
  "editor.formatOnSave": true,
  "editor.renderControlCharacters": false,
  "editor.renderLineHighlight": "all",
  "editor.rulers": [120],
  "editor.semanticHighlighting.enabled": true,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.snippetSuggestions": "inline",
  "errorLens.followCursor": "activeLine",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "afterDelay",
  "git.autofetch": true,
  "git.autoRepositoryDetection": false,
  "git.countBadge": "tracked",
  "git.fetchOnPull": true,
  "gitlens.blame.highlight.locations": ["gutter", "overview", "line"],
  "gitlens.blame.toggleMode": "window",
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "go-template.patterns": [
    "**/*.tpl",
    "**/*.gotpl",
    "**/*.thtml",
    "**/*.tmpl",
    "**/*.gohtml",
    "**/*.gtpl"
  ],
  "go.autocompleteUnimportedPackages": true,
  "go.buildOnSave": "package",
  "go.coverageOptions": "showCoveredCodeOnly",
  "go.delveConfig": {
    "dlvLoadConfig": {
      "maxArrayValues": 1000,
      "maxStringLen": 5000
    }
  },
  "go.editorContextMenuCommands": {
    "addImport": true,
    "addTags": true,
    "debugTestAtCursor": true,
    "fillStruct": true,
    "generateTestForFile": false,
    "generateTestForFunction": true,
    "generateTestForPackage": false,
    "playground": true,
    "removeTags": false,
    "testAtCursor": true,
    "testCoverage": true,
    "testFile": false,
    "testPackage": false,
    "toggleTestFile": true
  },
  "go.enableCodeLens": {
    "runtest": true
  },
  "go.formatTool": "gofmt",
  "go.lintFlags": ["--fast"],
  "go.lintTool": "golangci-lint",
  "go.liveErrors": {
    "delay": 500,
    "enabled": false
  },
  "go.playground": {
    "openbrowser": false,
    "run": true,
    "share": true
  },
  "go.testFlags": ["-v", "-timeout=0s"],
  "go.toolsManagement.autoUpdate": true,
  "go.useCodeSnippetsOnFunctionSuggest": true,
  "go.useLanguageServer": true,
  "gopls": {
    "ui.navigation.importShortcut": "Definition"
  },
  "javascript.preferences.useAliasesForRenames": false,
  "javascript.suggest.completeFunctionCalls": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  // "jest.enableInlineErrorMessages": true,
  // "jest.pathToJest": "",
  "kite.codefinder.enableLineDecoration": false,
  "kite.showWelcomeNotificationOnStartup": false,
